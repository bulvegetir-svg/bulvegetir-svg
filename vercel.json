{
  "github": {
    "enabled": false
  },

  "buildCommand": "npm run build",
  "outputDirectory": "dist",

  "rewrites": [
    { "source": "/support",  "destination": "/support/index.html" },
    { "source": "/support/", "destination": "/support/index.html" }
  ],

  "cleanUrls": true,
  "trailingSlash": false,

  "builds": [
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ]
}
