{
  "name": "bulvegetir-svg",
  "version": "0.1.1",
  "scripts": {
    "clean": "rimraf dist",
    "styles": "sass src/styles/style.scss dist/style.css --no-source-map && postcss dist/style.css -o dist/style.min.css",
    "scripts": "rollup -c",
    "html": "eleventy --config=.eleventy.js",
    "build": "npm-run-all clean styles scripts html",
    "start": "eleventy --serve --config=.eleventy.js"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "autoprefixer": "^10.4.24",
    "cssnano": "^7.1.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "rimraf": "^5.0.5",
    "rollup": "^4.12.0",
    "sass": "^1.77.0"
  }
}
